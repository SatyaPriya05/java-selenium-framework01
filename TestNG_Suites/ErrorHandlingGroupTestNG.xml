<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="TestNG Groups Test">

<groups>
	<run>
		<include name="Error Handling"/>
	</run>
</groups>

<listeners>
<listener class-name="testComponents.Listeners"/>
</listeners>

  <test thread-count="5" name="Create Account Errors Tests">
    <classes>
      <class name="tests.CreateAccount_FunctionalTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Sign In Errors Tests">
    <classes>
      <class name="tests.SignIn_FunctionalTest"/>
    </classes>
  </test>
  
 <!-- Test -->

  
</suite> <!-- Suite -->
